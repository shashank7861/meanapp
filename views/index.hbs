    </div>  
    <script>
      function initialize() {
        var fenway = {lat: 28.7041, lng: 77.1025};
        var map = new google.maps.Map(document.getElementById('maps'), {
          center: fenway,
          zoom: 16
        });
        var panorama = new google.maps.StreetViewPanorama(
            document.getElementById('pano'), {
              position: fenway,
              pov: {
                heading: 34,
                pitch: 10
              }
            });
        map.setStreetView(panorama);
      }
    </script>

