<h1>{{ title }}</h1>
    <div class="row">
      <div class="col-lg-12">
        <h3>Basic Information</h3>
        <hr>
        <div class="row">
          <div class="col-lg-6">
            <div class="form-group">
                Name<label class="form-control" type="text" id="name" value="" name="name">{{session.name}}</label>
            </div>
          </div>
          <div class="col-lg-6">
            <div class="form-group">
                Contact<label class="form-control" type="text" id="contact" value="" name="contact">{{session.contact}}</label>
            </div>
          </div>
        </div>
        <div class="form-group">
            Email<label class="form-control" type="text" id="email" value="" name="email">{{session.email}}</label>
        </div>
        <h3>Personal Details</h3>
        <hr>
          <form class="form" action="/create-profile/{{session._id}}" method="post">
            <div class="row">
              <div class="col-lg-6">
                <div class="form-group">
                    Address<textarea class="form-control" id="address" name="address" cols="2" rows="5">{{profile.address}}</textarea>
                </div>
              </div>
              <div class="col-lg-6">
                <div class="form-group">
                    City<input class="form-control" id="city" name="city" value="{{profile.city}}">
                </div>
                <div class="form-group">
                    State<input class="form-control" id="state" name="state" value="{{profile.state}}">
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-lg-6">
                <div class="form-group">
                    Country<input class="form-control" id="country" name="country" value="{{profile.country}}">
                </div>
              </div>
              <div class="col-lg-6">
                <div class="form-group">
                    Pin Code<input class="form-control" id="pin" name="pin" value="{{profile.pin}}">
                </div>
              </div>
            </div>
            <button class="btn btn-success" type="submit">Update</button>
          </form>
      </div>
    </div>
